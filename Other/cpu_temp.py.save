#!/usr/bin/python
import time, sys, json
from random import randint, seed
import CPU
seed(1)
while(True):
    lognumber = randint(0, 100)
    logdict = {
        'temperature': lognumber
    }
    logstring = json.dumps(logdict)
    with open("cpu_log.log", "a") as logfile:
        logfile.write(logstring + "\n")
    logfile.close()
    time.sleep(3)
sys.exit()
